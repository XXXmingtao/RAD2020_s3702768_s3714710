<% provide(:title, @user.name) %>
<div class="row">
    <aside class="col-md-4">
        <section class="user_info">
        <h1>
            <%= gravatar_for @user %>
            <div><%= @user.name %></div>
            <div><%= @user.email %></div>
            <div><%= @user.created_at %></div>
            <div><%= @user.updated_at %></div>
        </h1>
        </section>
    </aside>
</div>
